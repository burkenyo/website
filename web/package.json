{
  "name": "sammo-ga-website",
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "vite --open",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "type-check": "vue-tsc --build --force",
    "posttype-check": "rm vite.config.d.ts vite.config.js scripts/*.d.ts scripts/*.js # HACK to remove extraneous files caused by vite-ssg and having to include noEmit: false in tsconfig.config.json. When the underlying issue is resolved, remove this.",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "run-p test-only type-check",
    "test-only": "vitest run",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage",
    "host": "run-s build host-only",
    "host-only": "http-server dist -c-1 -a localhost -o"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "pinia": "^2.2.0",
    "vue": "^3.4.35",
    "vue-router": "^4.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.7",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-emoji": "^3.0.1",
    "@types/node": "~20.14.14",
    "@types/plotly.js": "^2.33.3",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/compiler-core": "^3.4.35",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/head": "^2.0.0",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "http-server": "^14.1.1",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-image-figures": "^2.1.1",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rollup": "^4.19.2",
    "typescript": "^5.5.4",
    "unplugin-vue-markdown": "^0.26.2",
    "vexflow": "^4.2.5",
    "vite": "^5.3.5",
    "vite-plugin-pages": "^0.32.3",
    "vite-ssg": "^0.23.8",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  }
}
